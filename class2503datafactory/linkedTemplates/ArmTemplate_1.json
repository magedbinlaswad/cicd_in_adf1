						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> source1",
						"source1 aggregate(groupBy(id),",
						"     name = first(name),",
						"          gender = first(gender),",
						"          salary = first(salary)) ~> aggregate1",
						"aggregate1 sort(asc(id, true)) ~> sort1",
						"sort1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          {Sale Key} as string,",
						"          {Invoice Date Key} as string,",
						"          {Delivery Date Key} as string,",
						"          Quantity as string,",
						"          {Unit Price} as string,",
						"          {Tax Rate} as string,",
						"          {Total Excluding Tax} as string,",
						"          Profit as string",
						"     ),",
						"     partitionFileNames:['groupbyid.csv'],",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     partitionBy('hash', 1)) ~> sink1"
