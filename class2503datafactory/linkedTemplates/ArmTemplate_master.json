{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "class2503datafactory"
		},
		"ls_DataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_DataLakeStorage1'"
		},
		"ls_database2503_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_database2503'"
		},
		"ls_DataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://class2503datalake.dfs.core.windows.net/"
		},
		"ls_database2503_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "lssqlserver2503.database.windows.net"
		},
		"ls_database2503_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mysql2503"
		},
		"ls_database2503_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "maged"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "class2503datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_DataLakeStorage1_accountKey": {
						"value": "[parameters('ls_DataLakeStorage1_accountKey')]"
					},
					"ls_database2503_password": {
						"value": "[parameters('ls_database2503_password')]"
					},
					"ls_DataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('ls_DataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_database2503_properties_typeProperties_server": {
						"value": "[parameters('ls_database2503_properties_typeProperties_server')]"
					},
					"ls_database2503_properties_typeProperties_database": {
						"value": "[parameters('ls_database2503_properties_typeProperties_database')]"
					},
					"ls_database2503_properties_typeProperties_userName": {
						"value": "[parameters('ls_database2503_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}